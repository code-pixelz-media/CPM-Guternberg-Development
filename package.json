{
	"name": "cpm-advance-block",
	"version": "1.0.0",
	"description": "codepizelzmedia's Custom Gutenberg Blocks made for WordPress easy.",
	"author": "codepizelzmedia",
	"license": "GPL-2.0-or-later",
	"main": "blocks.js",
	"scripts": {
		"build:admin": "wp-scripts build src/settings.js",
		"build:public": "wp-scripts build src/public.js",
		"build:blocks": "wp-scripts build src/blocks.js",
		"build": "npm-run-all --parallel build:admin build:public build:blocks",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start:admin": "wp-scripts start src/settings.js",
		"start:public": "wp-scripts start src/public.js",
		"start:blocks": "wp-scripts start src/blocks.js",
		"start": "npm-run-all --parallel start:admin start:public start:blocks",
